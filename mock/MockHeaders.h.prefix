template<typename R, typename... Args>
template<typename F>
MockResultHolder<R(Args...)>::MockResultHolder(std::string const& name, F&& original)
    : name(name)
    , original(std::move(original))
    , next(0)
    , errorMode(false)
{}

template<typename R, typename... Args>
std::string const& MockResultHolder<R(Args...)>::getName() const
{
    return name;
}

template<typename R, typename... Args>
std::size_t MockResultHolder<R(Args...)>::expectInit(Action action, std::size_t index, std::size_t count, bool last, Ret&& value, Required required, Order order)
{
    switch (action)
    {
        case AddExtra:
            if (!errorMode) {
                output.clear();
                errorMode = true;
            }
            // Fall through to Adding
        case Adding:
        {
            std::size_t indexOrder = -1;
            if (order == Order::InOrder) {
                indexOrder = index++;
            }
            output.emplace_back(OutputInfo{indexOrder, count, last, std::move(value), required});
            break;
        }
        case Cleaning:
        {
            if (index == 0) {
                std::size_t count = 0;
                for (std::size_t loop = next; loop < output.size(); ++loop) {
                    OutputInfo& outputInfo = output[loop];
                    Required&   required   = std::get<4>(outputInfo);
                    count += (required == Required::Yes) ? 1: 0;
                }
                EXPECT_EQ(count, 0)
                        << "Function: " << getName() << " did not use all expected calls. "
                        << (output.size() - next) << " left unused";
            }
            reset();
            break;
        }
    }
    return index;
}

template<typename R, typename... Args>
void MockResultHolder<R(Args...)>::reset()
{
    output.clear();
    input.clear();
    next = 0;
    errorMode = false;
}

template<typename R, typename... Args>
R MockResultHolder<R(Args...)>::call(TA_Test& parent, Args&&... args)
{
    //std::cerr << "Calling: " << getName() << "\n";
    while (next == output.size()) {
        if (!parent.unexpected()) {
            // There are no more changes that can be made.
            break;
        }
    }

    if (next < input.size() && static_cast<bool>(input[next])) {
        std::tuple<Args...>&    expected = *input[next];
        EXPECT_EQ(std::tie(args...), expected) << "Function: " << getName() << " Input to function does not match expectations";
    }
    if (next < output.size()) {
        OutputInfo& outputInfo = output[next];
        std::size_t   nextCallIndex = std::get<0>(outputInfo);
        std::size_t&  callCount     = std::get<1>(outputInfo);
        bool          lastCall      = std::get<2>(outputInfo);

        --callCount;
        if (callCount == 0) {
            ++next;
        }

        EXPECT_TRUE(parent.mockCalled(nextCallIndex, lastCall)) << "Function: " << getName() << "Called out of order";
        if constexpr (std::is_same_v<R, void>) {
            return;
        }
        else {
            return std::get<3>(outputInfo);
        }
    }
    EXPECT_TRUE(next < output.size()) << "Function: " << getName() << " called more times than expected";
    throw std::runtime_error("Failed");
    // return original(std::forward<Args>(args)...);
}

