template<typename MockObject>
void TA_Code<MockObject>::saveCall(Ret&& result)
{
    if (parent.codeBlocks.empty()) {
        parent.codeBlocks.emplace_back();
    }
    callSaved = true;
    parent.codeBlocks.back().saveCall(action, callCount, std::move(result), insert, order);
}

