template<typename MockObject, typename Result>
void TA_Object::saveCall(MockObject& action, std::size_t callCount, Result&& result, Insert insert, Order order)
{
    if (insert == Insert::Inject) {
        reset.emplace_back([mockObject = &action](){mockObject->reset();});
    }
    extraCode.add(order, [mockObject = &action, c = callCount, r = std::forward<Result>(result), o = order](Action action, std::size_t index, bool last) mutable {
        return mockObject->expectInit(action, index, c, last, std::move(r), Required::Yes, o);
    });
}

